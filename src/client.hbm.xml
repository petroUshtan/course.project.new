<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="objects.Client" table="clients">
        <id name="clientId" type="java.lang.Long" column="clientId">
            <!--<generator class="native"></generator>-->
        </id>
        <property name="clientFIO" column="clientFIO" type="java.lang.String"/>
        <property name="registerTime" column="productPrice" type="java.util.Date"/>
    </class>
</hibernate-mapping>